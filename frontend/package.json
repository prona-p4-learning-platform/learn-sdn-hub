{
  "name": "frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "create-static-backend": "npx shx rm -rf ../backend/static && npx shx mkdir -p ../backend/static && npx shx cp -rf ./build/* ../backend/static/",
    "dev": "vite",
    "format": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "lint": "eslint . --cache --ext ts,tsx",
    "postinstall": "npx shx mkdir -p ./public/dist && npx shx cp -f ../node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist || npx shx cp -f ./node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist",
    "preview": "vite preview",
    "type-check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@monaco-editor/react": "^4.4.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.20",
    "destr": "^2.0.3",
    "guacamole-common-js": "1.4.0-a",
    "js-base64": "^3.7.7",
    "lodash-es": "^4.17.21",
    "mermaid": "^10.9.1",
    "monaco-editor": "^0.33.0",
    "monaco-editor-workers": "^0.33.0",
    "monaco-languageclient": "^2.1.0",
    "notistack": "^3.0.1",
    "ofetch": "^1.3.4",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.23.1",
    "vscode-ws-jsonrpc": "^1.0.2",
    "xterm": "5.2.1",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-serialize": "^0.11.0",
    "y-monaco": "^0.1.5",
    "y-webrtc": "^10.3.0",
    "y-websocket": "^2.0.3",
    "yjs": "^13.6.18",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/guacamole-common-js": "1.3.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
