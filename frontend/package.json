{
  "name": "frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "create-static-backend": "npx shx rm -rf ../backend/static && npx shx mkdir -p ../backend/static && npx shx cp -rf ./build/* ../backend/static/",
    "dev": "vite",
    "format": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "lint": "eslint . --cache",
    "postinstall": "npx shx mkdir -p ./public/dist && npx shx cp -f ../node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist || npx shx cp -f ./node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist",
    "preview": "vite preview",
    "type-check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.0",
    "@monaco-editor/react": "^4.4.5",
    "@mui/icons-material": "^6.2.1",
    "@mui/lab": "^6.0.0-beta.20",
    "@mui/material": "^6.2.1",
    "destr": "^2.0.3",
    "guacamole-common-js": "1.4.0-a",
    "js-base64": "^3.7.7",
    "lodash-es": "^4.17.21",
    "mermaid": "^11.10.0",
    "monaco-editor": "^0.33.0",
    "monaco-editor-workers": "^0.33.0",
    "monaco-languageclient": "^2.1.0",
    "notistack": "^3.0.1",
    "ofetch": "^1.4.1",
    "oidc-client-ts": "^3.1.0",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router": "^7.6.2",
    "vscode-ws-jsonrpc": "^1.0.2",
    "xterm": "5.2.1",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-serialize": "^0.11.0",
    "y-monaco": "^0.1.6",
    "y-webrtc": "^10.3.0",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.20",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/guacamole-common-js": "1.3.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.4.1"
  }
}
