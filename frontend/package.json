  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/guacamole-common-js": "^1.3.2",
    "nth-check": "^2.1.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack",
    "nth-check": "$nth-check"
  },
  "scripts": {
    "copy:monaco-worker": "npx shx mkdir -p ./public/dist && npx shx cp -f ../node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist || npx shx cp -f ./node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist",
    "start": "npm run copy:monaco-worker && react-app-rewired start",
    "build": "npm run copy:monaco-worker &&  react-app-rewired build",
    "create-static-backend": "npx shx rm -rf ../backend/static && npx shx mkdir -p ../backend/static && npx shx cp -rf ./build/* ../backend/static/",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
