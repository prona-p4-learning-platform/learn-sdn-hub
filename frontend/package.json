{
  "name": "frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "create-static-backend": "npx shx rm -rf ../backend/static && npx shx mkdir -p ../backend/static && npx shx cp -rf ./build/* ../backend/static/",
    "dev": "vite",
    "format": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "lint": "eslint . --cache --ext ts,tsx",
    "postinstall": "npx shx mkdir -p ./public/dist && npx shx cp -f ../node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist || npx shx cp -f ./node_modules/monaco-editor-workers/dist/workers/*.js ./public/dist",
    "preview": "vite preview",
    "type-check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@monaco-editor/react": "^4.4.5",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "destr": "^2.0.3",
    "guacamole-common-js": "1.4.0-a",
    "js-base64": "^3.7.6",
    "mermaid": "^9.4.0",
    "monaco-editor": "^0.33.0",
    "monaco-editor-workers": "^0.33.0",
    "monaco-languageclient": "^2.1.0",
    "ofetch": "^1.3.3",
    "path": "^0.12.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^5.3.2",
    "vscode-ws-jsonrpc": "^1.0.2",
    "web-vitals": "^2.1.4",
    "xterm": "^5.2.0",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-serialize": "^0.11.0",
    "y-monaco": "^0.1.5",
    "y-webrtc": "^10.3.0",
    "y-websocket": "^1.5.3",
    "yjs": "^13.6.11",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/guacamole-common-js": "1.3.2",
    "@types/node": "^18.19.10",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.12"
  }
}
